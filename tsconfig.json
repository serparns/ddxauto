{
  "compilerOptions": {
    "allowJs": false,
    "noImplicitAny": false,
    "target": "esnext",
    "moduleResolution": "node",
    "strict": false,
    "module": "commonjs",
    "sourceMap": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "types": [
      "node",
      "@wdio/globals/types",
      "webdriverio/async",
      "mocha",
      "expect-webdriverio"
    ],
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "paths": {
      "@requests/*": [
        "requests/*"
      ],
      "@libs/*": [
        "libs/*"
      ],
      "@entities/*": [
        "entities/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@data/*": [
        "data/*"
      ],
      "@api": [
        "api.json"
      ]
    }
  }
}